package  {		import flash.display.Sprite;	import flash.display.Shape;		public class MapUnit extends Sprite {				public var w:Number = 2;		public var h:Number = 4;		public var block:Shape;				public function MapUnit(population:Number) {			block = new Shape;			if (population == 0) {				block.graphics.beginFill(0x333333);			} else {				block.graphics.beginFill(0xFFFFFF);			}			block.graphics.drawRect(0, 0, w, h);			block.graphics.endFill();			addChild(block);		}	}	}